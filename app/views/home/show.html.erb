<h2> Resumo do Usuario</h2>

    <%- model_class = User -%>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @user.email %></dd>
  <dd><%= @user.password %></dd>
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @user.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:role) %>:</strong></dt>
  <dd><%= @user.role %></dd>
</dl>



<%= link_to t('.Voltar', :default => t("helpers.links.Voltar")),
    home_manipulationusers_path, :class => 'btn btn-default', style:"color: blue;"  %>  
